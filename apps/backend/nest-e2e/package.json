{
  "name": "@devapp/nest-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@devapp/nest"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/backend/nest-e2e/jest.config.cts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@devapp/nest:build",
          "@devapp/nest:serve"
        ]
      }
    }
  }
}
